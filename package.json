{
  "private": true,
  "author": "Rolf Timmermans",
  "name": "tfw",
  "version": "8.1.0",
  "main": "dist/application.js",
  "types": "dist/application.d.ts",
  "license": "UNLICENSED",
  "type": "module",
  "engines": {
    "node": ">=8"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "tsc",
    "build": "rm -rf lib && tsc --project tsconfig.build.json"
  },
  "dependencies": {
    "ajv": "^8.6",
    "content-type": "^1.0.2",
    "cookies": "^0.8.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/content-type": "^1.1.3",
    "@types/cookies": "^0.7.7",
    "@types/mocha": "^9.0",
    "@types/node": "^16.11",
    "app-module-path": "^2.2.0",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.9.0",
    "babel-plugin-syntax-async-generators": "^6.9.0",
    "babel-plugin-transform-class-properties": "^6.9.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.9.0",
    "babel-plugin-transform-export-extensions": "^6.9.0",
    "babel-plugin-transform-flow-strip-types": "^6.9.0",
    "babel-plugin-transform-function-bind": "^6.9.0",
    "babel-plugin-transform-strict-mode": "^6.9.0",
    "benchmark": "^2.1.0",
    "chai": "^4.1.0",
    "mocha": "^9.0",
    "nyc": "^15.0",
    "source-map-support": "^0.5.11",
    "timekeeper": ">= 0",
    "ts-node": "^10.0",
    "typescript": "^4.9"
  },
  "mocha": {
    "extension": "js",
    "spec": "test/unit/**/*.js",
    "use_strict": true,
    "loader": "ts-node/esm",
    "require": ["test"],
    "recursive": true,
    "timeout": 4000,
    "exit": true,
    "reporter": "list"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  }
}
